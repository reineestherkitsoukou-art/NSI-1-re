from __future__ import annotations
from random import randint

# Pour activer / dÃ©sactiver les tests
test = True

a = 4.2
assert a < 5
assert a > 3, "a doit être plus grand que 3"
assert type(a) is float, "a doit être doit être un flotant"

def power(a:int,n:int) ->int:
    """
    calcul la puissance (n) d'un nombre a

    """
    assert n >= 0
    r = 1
    for i in range(n):
        r = a*r
    assert r == a**n
    return r

if test== True:
    assert power(3,2) == 9
    assert power(7,7) == 823543
    assert power(1552,5423) == 
    assert power(12,0) == 1
    assert power(0,154) == 0
    assert power(-45,3) == -91125

table_alea = [randint(0,1000) for _ in range(1000)]
  
# Fonction à  tester  
def recherche_seq_pos(v:int,t:list[int]) -> int:
    """
    Renvoie la position de la première occurence de v dans t ou
    -1 en cas d'echec
    """
    
    p = 0
    while p < len(t):
        if v == t[p]:
            return p
        p+=1
    return -1

# Jeu de tests :
    assert recherche_seq_pos(-12,table_alea)
    assert recherche_seq_pos()
